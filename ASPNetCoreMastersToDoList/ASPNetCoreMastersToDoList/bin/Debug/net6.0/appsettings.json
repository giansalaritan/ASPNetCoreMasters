{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "AllowedHosts": "*",
  "Authentication": {
    "JWT": {
      "SecurityKey": "ASPNetCoreMasters Key",
      "Issuer": "https://localhost:5001",
      "Audience": "https://localhost:5001"
    }
  },
  "ConnectionStrings": {
    "Default": "Data Source=(localdb)\\mssqllocaldb;Integrated Security=True"
  }
}
